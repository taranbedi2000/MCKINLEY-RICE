#include<iostream>
using namespace std;
int main()
{
int a[100],i,n,minimum,maximum;

cin >> n;

for(i=0;i<n;++i)
cin >> a[i];

maximum=minimum=a[0];

for(i=1;i<n;++i)
{
if(a[i]>maximum)
maximum=a[i];

if(a[i]<minimum)
minimum=a[i];
}

cout <<  minimum << endl;
cout << maximum << endl;

return 0;
}
